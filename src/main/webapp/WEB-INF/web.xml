<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Index</servlet-name>
  	<display-name>Index</display-name>
  	<description></description>
  	<servlet-class>servlets.Index</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Compte</servlet-name>
  	<display-name>Compte</display-name>
  	<description></description>
  	<servlet-class>servlets.Compte</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Connexion</servlet-name>
  	<display-name>Connexion</display-name>
  	<description></description>
  	<servlet-class>servlets.Connexion</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Index</servlet-name>
  	<url-pattern>/Index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Compte</servlet-name>
  	<url-pattern>/Compte</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Connexion</servlet-name>
  	<url-pattern>/Connexion</url-pattern>
  </servlet-mapping>
</web-app>